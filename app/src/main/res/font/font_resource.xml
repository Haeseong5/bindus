<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android">
    <font-family
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        tools:ignore="UnusedAttribute">

        <!-- normal -->
        <font
            android:font="@font/nanumsquare_reqular"
            android:fontStyle="normal"
            android:fontWeight="400"
            app:font="@font/nanumsquare_reqular"
            app:fontStyle="normal"
            app:fontWeight="400" />

        <!-- light -->
        <font
            android:font="@font/nanumsquare_light"
            android:fontStyle="italic"
            android:fontWeight="400"
            app:font="@font/nanumsquare_light"
            app:fontStyle="italic"
            app:fontWeight="400" />

        <!--extra bold -->
        <font
            android:font="@font/nanumsquare_extra_bold"
            android:fontStyle="normal"
            android:fontWeight="700"
            app:font="@font/nanumsquare_extra_bold"
            app:fontStyle="normal"
            app:fontWeight="400" />
        <font
            android:font="@font/nanumsquare_bold"
            android:fontStyle="normal"
            android:fontWeight="700"
            app:font="@font/nanumsquare_bold"
            app:fontStyle="normal"
            app:fontWeight="400" />
    </font-family>
</font-family>